Cell[TextData[{
 StyleBox["Section 1.4, #4: ",
  FontWeight->"Bold"],
 "Let ",
 StyleBox["S",
  FontSlant->"Italic"],
 " be a nonempty set of real numbers that is bounded above and let ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Beta]", "=", 
    RowBox[{"sup", "(", "S", ")"}]}], TraditionalForm]]],
 ".  Suppose that ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Beta]", "\[NotElement]", "S"}], TraditionalForm]]],
 ".  Prove that for each ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Epsilon]", ">", "0"}], TraditionalForm]]],
 ", the set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{"x", "\[Element]", 
     RowBox[{"S", ":", 
      RowBox[{"x", ">", 
       RowBox[{"\[Beta]", "-", "\[Epsilon]"}]}]}]}], "}"}], TraditionalForm]]],
 " is infinite."
}], "Section",
 CellChangeTimes->{{3.431078603356449*^9, 3.431078605954555*^9}, {
  3.68284424400935*^9, 3.682844301445302*^9}}],

