  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["y", 
      RowBox[{" ", "\[Prime]"}]], "=", 
     RowBox[{"b", " ", "y"}]}], ",", " ", 
    RowBox[{
     RowBox[{"y", "(", "0", ")"}], "=", "a"}]}], TraditionalForm]],
  FontWeight->"Plain"],
 StyleBox[".  (3) ",
