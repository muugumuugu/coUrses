Cell[TextData[{
 "The MVT for a given function ",
 StyleBox["f",
  FontSlant->"Italic"],
 " and given interval ",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", 
    RowBox[{"a", ",", "b"}], "]"}], TraditionalForm]],ExpressionUUID->
  "af0cca96-4498-4dc8-bfdc-0d9542fd2242"],
 " follows by applying Rolle's Theorem to the function ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"g", "(", "x", ")"}], "=", 
    RowBox[{
     RowBox[{"f", "(", "x", ")"}], "-", 
     RowBox[{"L", "(", "x", ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "e55c4bd1-50da-435e-81a3-7b8073984755"],
 ", where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"L", "(", "x", ")"}], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{
        RowBox[{"f", "(", "b", ")"}], "-", 
        RowBox[{"f", "(", "a", ")"}]}], 
       RowBox[{"b", "-", "a"}]], 
      RowBox[{"(", 
       RowBox[{"x", "-", "a"}], ")"}]}], "+", 
