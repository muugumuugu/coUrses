Cell[TextData[{
 StyleBox["The Constant Function Theorem:",
  FontWeight->"Bold"],
 "  ",
 StyleBox["If ",
  FontWeight->"Plain"],
 StyleBox["f",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox["  is a continuous function on the closed interval [a,b] and a \
differentiable function on the open interval (a,b) and if ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["f", 
      RowBox[{" ", "\[Prime]"}]], "(", "x", ")"}], "=", "0"}], 
   TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "02950810-b5b6-45f3-ba65-d1f80035fb5a"],
 StyleBox[" for all ",
  FontWeight->"Plain"],
 StyleBox["x",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" in the open interval (a,b), then ",
  FontWeight->"Plain"],
 StyleBox["f",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox["  is a constant on the closed interval [a,b].",
  FontWeight->"Plain"]
}], "Subsubsection",
 CellChangeTimes->{{3.427819212378941*^9, 3.4278192435047197`*^9}, {
   3.427819277069644*^9, 3.427819353021834*^9}, 
   3.4280600958043756`*^9},ExpressionUUID->"39d411ab-174b-443b-be08-\
67dde9a68694"],

Cell[TextData[{
 StyleBox["Antiderivatives of the same function differ by constants:",
  FontWeight->"Bold"],
 "  ",
 StyleBox["Let ",
  FontWeight->"Plain"],
 StyleBox["f",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" and ",
  FontWeight->"Plain"],
 StyleBox["g",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" be two functions which are continuous on the closed interval \
[a,b] and differentiable on the open interval (a,b).  If ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["f", 
      RowBox[{" ", "\[Prime]"}]], "(", "x", ")"}], "=", 
    RowBox[{
     SuperscriptBox["g", 
      RowBox[{" ", "\[Prime]"}]], "(", "x", ")"}]}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "24f05828-426d-4e4a-9041-d5fcac540283"],
 StyleBox[" for all ",
  FontWeight->"Plain"],
 StyleBox["x",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" in the open interval (a,b), then there is a constant ",
  FontWeight->"Plain"],
 StyleBox["C",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" such that ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "x", ")"}], "=", 
    RowBox[{
     RowBox[{"g", "(", "x", ")"}], "+", "C"}]}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "53e6ef38-e5f6-4cef-a535-bd3698b85ced"],
 StyleBox[" for all ",
  FontWeight->"Plain"],
 StyleBox["x",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" in the closed interval [a,b].",
  FontWeight->"Plain"]
}], "Subsubsection",
 CellChangeTimes->{{3.427819373635366*^9, 
  3.427819513306798*^9}},ExpressionUUID->"92fc928c-8b84-4e95-bbb9-\
ef5d41dba0d5"],

Cell[TextData[{
 StyleBox["Other applications:",
  FontWeight->"Bold"],
 "  ",
 StyleBox["(1) ",
  FontWeight->"Plain"],
 StyleBox[ButtonBox["the first derivative test",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/First_derivative_test"], None}],
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 StyleBox[",",
  FontWeight->"Plain"],
 " ",
 StyleBox["(2) ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{"a", " ", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{"b", " ", "x"}]]}]}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "51225a50-78ff-4171-b112-076acb023360"],
 StyleBox[" is the \"general solution\" of the differential equation ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["y", 
     RowBox[{" ", "\[Prime]"}]], "=", 
    RowBox[{"b", " ", "y"}]}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "4ea2b34d-e795-4bd1-895f-a087de65f32f"],
 StyleBox[".  Also, this function is the unique solution of the \"initial \
value problem\" ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["y", 
      RowBox[{" ", "\[Prime]"}]], "=", 
     RowBox[{"b", " ", "y"}]}], ",", " ", 
    RowBox[{
     RowBox[{"y", "(", "0", ")"}], "=", "a"}]}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "16cf1799-f5e6-4b61-a6e7-58854e2bb812"],
 StyleBox[".  (3) ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{
     RowBox[{"a", " ", 
      RowBox[{"cos", "(", 
       RowBox[{"k", " ", "x"}], ")"}]}], "+", 
     RowBox[{"b", " ", 
      RowBox[{"sin", "(", 
       RowBox[{"k", " ", "x"}], ")"}]}]}]}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "cea13618-5a46-4edf-82ea-c993e66fea6d"],
 StyleBox[" is the \"general solution\" of the differential equation ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["y", 
     RowBox[{" ", "\[DoublePrime]"}]], "=", 
    RowBox[{
     RowBox[{"-", 
      SuperscriptBox["k", "2"]}], "y"}]}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "9237bbe2-4198-416d-b02a-baaa95fc86b9"],
 ".  ",
 StyleBox["Also, this function is the unique solution of the initial value \
problem",
  FontWeight->"Plain"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["y", 
      RowBox[{" ", "\[DoublePrime]"}]], "=", 
     RowBox[{
      RowBox[{"-", 
       SuperscriptBox["k", "2"]}], "y"}]}], ",", " ", 
    RowBox[{
     RowBox[{"y", "(", "0", ")"}], "=", "a"}], ",", " ", 
    RowBox[{
     RowBox[{
      RowBox[{"y", "'"}], 
      RowBox[{"(", "0", ")"}]}], " ", "=", " ", 
     RowBox[{"b", " ", "k"}]}]}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "f4373db8-df6f-4447-8588-3bd47dde25f5"],
 ". ",
 StyleBox["  (4) The ",
  FontWeight->"Plain"],
 StyleBox[ButtonBox["Fundamental Theorem of Calculus",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Fundamental_theorem_of_calculus"], 
    None}],
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 StyleBox[".  And the list goes on and on and on...  ",
  FontWeight->"Plain"]
}], "Subsubsection",
 CellChangeTimes->{{3.427820567081977*^9, 3.4278207746226*^9}, {
  3.428060114005352*^9, 3.428060212992427*^9}, {3.428173506337448*^9, 
  3.4281735264643097`*^9}, {3.4281737687614527`*^9, 3.4281737803533497`*^9}, {
  3.4281738288357*^9, 
  3.428173828836656*^9}},ExpressionUUID->"bb58206c-e67d-4f29-b2bb-\
2e2f49547d8f"]
}, Open  ]],

