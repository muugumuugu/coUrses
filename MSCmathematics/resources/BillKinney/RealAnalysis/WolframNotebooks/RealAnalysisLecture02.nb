Cell[TextData[{
 "A Fundamental Result in Calculus:  ",
 StyleBox[ButtonBox["The Mean Value Theorem",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Mean_value_theorem"], None}],
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 " (\"MVT!  MVT!  MVT!\") (Note:  you'll want to come back to this section of \
this notebook often in the next couple months)"
}], "Section",
 CellChangeTimes->{{3.427813715486308*^9, 3.427813734324635*^9}, {
  3.427823265994441*^9, 3.427823266007557*^9}, {3.428062197544775*^9, 
  3.428062235062221*^9}, {3.5552367841984577`*^9, 
  3.5552367884594307`*^9}},ExpressionUUID->"40e54763-ea42-4c57-b4ee-\
a9461caedaa6"],

Cell[TextData[{
 StyleBox["Statement of the ",
  FontWeight->"Bold"],
 StyleBox[ButtonBox["MVT",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Mean_value_theorem"], None}],
  FontWeight->"Bold",
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 ":  ",
 StyleBox["If ",
  FontWeight->"Plain"],
 StyleBox["f",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox["  is a continuous function on the closed interval [a,b] and a \
differentiable function on the open interval (a,b), then there exists a \
number c \[Element] (a,b) such that  \n\t\t\t\t",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["f", 
      RowBox[{" ", "\[Prime]"}]], "(", "c", ")"}], "=", 
    FractionBox[
     RowBox[{
      RowBox[{"f", "(", "b", ")"}], " ", "-", " ", 
      RowBox[{"f", "(", "a", ")"}]}], 
     RowBox[{"b", " ", "-", " ", "a"}]]}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->"27257a5f-3e91-42e3-b348-f21c214d45f2"]
}], "Subsection",
 CellChangeTimes->{{3.427818432719614*^9, 3.427818536254238*^9}, {
  3.4278192315977592`*^9, 3.4278192330315104`*^9}, {3.428062261670611*^9, 
  3.4280622686861477`*^9}, {3.4281734256448593`*^9, 
  3.428173425652426*^9}},ExpressionUUID->"ea0d924a-55d2-4d07-a0aa-\
b78c4ead9c0c"],

