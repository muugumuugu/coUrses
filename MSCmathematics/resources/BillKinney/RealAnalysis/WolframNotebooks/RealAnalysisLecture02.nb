Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "x", ")"}], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"k", "=", "0"}], "\[Infinity]"], 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox["3", "4"], ")"}], "k"], 
      RowBox[{"g", "(", 
       RowBox[{
        SuperscriptBox["4", "k"], "x"}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"5fc5a565-362d-4eb0-b2bb-8f7f4f959892"],
 ".  It turns out that ",
 StyleBox["f",
  FontSlant->"Italic"],
 " is ",
 StyleBox["continuous everywhere but differentiable nowhere",
  FontSlant->"Italic"],
 "!"
}], "Text",
 CellChangeTimes->{{3.427737855887424*^9, 
  3.4277379077564497`*^9}},ExpressionUUID->"604851b8-8cd4-47ae-9448-\
7d32dfef3faa"],

