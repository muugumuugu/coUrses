Cell[TextData[{
 StyleBox["The ",
  FontWeight->"Bold"],
 StyleBox[ButtonBox["Definition of Continuity",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Continuous_function#Cauchy_definition_.\
28epsilon-delta.29"], None}],
  FontWeight->"Bold",
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 StyleBox[": ",
  FontWeight->"Bold"],
 " ",
 StyleBox["A function ",
  FontWeight->"Plain"],
 StyleBox["f",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" defined on an interval containing a number ",
  FontWeight->"Plain"],
 StyleBox["c",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" is said to be ",
  FontWeight->"Plain"],
 "continuous at ",
 StyleBox["c",
  FontSlant->"Italic"],
 StyleBox[" if for all ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Epsilon]", ">", "0"}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "dfe8b470-47f1-4636-bb0b-724da700d810"],
 StyleBox[", there exists a number ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Delta]", ">", "0"}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "7191eee9-e457-4681-916d-4a7eec015b59"],
 StyleBox[" such that ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftBracketingBar]", 
     RowBox[{
      RowBox[{"f", "(", "x", ")"}], "-", 
      RowBox[{"f", "(", "c", ")"}]}], "\[RightBracketingBar]"}], "<", 
    "\[Epsilon]"}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "963f2b11-3144-48a3-b251-dfd9f949820e"],
 StyleBox[" for all ",
  FontWeight->"Plain"],
 StyleBox["x ",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox["in the interval which satisfy the condition that ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"0", "<", 
    RowBox[{"\[LeftBracketingBar]", 
     RowBox[{"x", "-", "c"}], "\[RightBracketingBar]"}], "<", "\[Epsilon]"}], 
   TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "0bbd26f8-688e-4729-af07-d3ecddfe46e4"],
 StyleBox[".  A function ",
  FontWeight->"Plain"],
 StyleBox["f",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" is said to be ",
  FontWeight->"Plain"],
 "continuous on an interval",
 StyleBox[" if it is continuous at every point in that interval.  In effect, \
continuity at a number ",
  FontWeight->"Plain"],
 StyleBox["c",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" says that ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["lim", 
      RowBox[{"x", "\[Rule]", "c"}]], 
     RowBox[{"f", "(", "x", ")"}]}], "=", 
    RowBox[{"f", "(", "c", ")"}]}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "f0c138d2-c4ff-4688-a8d2-0df8a73a0c5e"],
 StyleBox[" (assuming ",
  FontWeight->"Plain"],
 StyleBox["x",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" is close enough to ",
  FontWeight->"Plain"],
 StyleBox["c",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" to be in the domain of ",
  FontWeight->"Plain"],
 StyleBox["f ",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[").  In other words, the definition of continuity is inextricably \
linked to the concept of limits.      ",
  FontWeight->"Plain"]
}], "Subsubsection",
 CellChangeTimes->{{3.428060814260861*^9, 3.428061125375574*^9}, {
  3.428061241372839*^9, 3.428061241555183*^9}, {3.42817459811684*^9, 
  3.428174598131537*^9}},ExpressionUUID->"11afb6f0-db7c-45a7-8e59-\
fbd3a33a40b4"],

Cell[TextData[{
 StyleBox["The ",
  FontWeight->"Bold"],
 StyleBox[ButtonBox["Definition of Convergence of a Sequence",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Limit_of_a_sequence#Formal_definition"],
     None}],
  FontWeight->"Bold",
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 StyleBox[":",
  FontWeight->"Bold"],
 "  ",
 StyleBox["A sequence ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    SubscriptBox["x", "n"], "}"}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "328776fe-cc2f-4937-8e6e-6fab2c18cbec"],
 StyleBox[" is said to ",
  FontWeight->"Plain"],
 "converge to a number ",
 StyleBox["L",
  FontSlant->"Italic"],
 StyleBox[" if for all ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Epsilon]", ">", "0"}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "66379ae1-b89a-4ffc-be17-edd636465ae3"],
 StyleBox[", there exists a positive integer ",
  FontWeight->"Plain"],
 StyleBox["N",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" such that ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftBracketingBar]", 
     RowBox[{
      SubscriptBox["x", "n"], "-", "L"}], "\[RightBracketingBar]"}], "<", 
    "\[Epsilon]"}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "83cfdb11-2b23-4a56-b73a-49cda404ddbe"],
 StyleBox[" for all positive integers ",
  FontWeight->"Plain"],
 StyleBox["n",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" satisfying the condition that ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "\[GreaterEqual]", "N"}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "9a431500-d94b-4687-9590-1a2a536ac62c"],
 StyleBox[".",
  FontWeight->"Plain"]
}], "Subsubsection",
 CellChangeTimes->{{3.42806118564123*^9, 3.428061324116948*^9}, {
  3.428174646617084*^9, 
  3.428174646633749*^9}},ExpressionUUID->"7f15d777-500d-42b7-add5-\
b06fcb718281"],

Cell[TextData[{
 StyleBox["The Definition of a Bounded Sequence:",
  FontWeight->"Bold"],
 "  ",
 StyleBox["A sequence ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    SubscriptBox["x", "n"], "}"}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "ca64eded-7f8a-4627-8734-3ab68a54c7c0"],
 StyleBox[" is ",
  FontWeight->"Plain"],
 "bounded",
 StyleBox[" if there exists a number ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"M", ">", "0"}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "58ed14e7-c1ca-440d-b5ab-6a6df9f2afce"],
 StyleBox[" such that ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftBracketingBar]", 
     SubscriptBox["x", "n"], "\[RightBracketingBar]"}], "<", "M"}], 
   TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "9cd818af-6ef1-4fab-afff-b662e38de57a"],
 StyleBox[" for all positive integers ",
  FontWeight->"Plain"],
 StyleBox["n",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[".",
  FontWeight->"Plain"],
 "  "
}], "Subsubsection",
 CellChangeTimes->{{3.428061366869833*^9, 
  3.428061567989402*^9}},ExpressionUUID->"294d7074-4576-420c-8682-\
d275aae2c04b"],

Cell[TextData[{
 StyleBox["The Definition of a ",
  FontWeight->"Bold"],
 StyleBox[ButtonBox["Subsequence",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Subsequence"], None}],
  FontWeight->"Bold",
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 StyleBox[" of a Given Sequence:",
  FontWeight->"Bold"],
 "  ",
 StyleBox["A ",
  FontWeight->"Plain"],
 "subsequence",
 StyleBox[" of a given sequence ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    SubscriptBox["x", "n"], "}"}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "b1579f5d-d56d-49bd-8632-5049544a280d"],
 StyleBox[" is a sequence made up of some of the terms of ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    SubscriptBox["x", "n"], "}"}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "169f4d93-82df-4466-aec2-04833ce09eda"],
 StyleBox[" in the same order they appear in the original sequence.  To be \
more precise, a subsequence of ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    SubscriptBox["x", "n"], "}"}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "81ba397c-c96a-4517-a44f-d698db34ba03"],
 StyleBox[" is any sequence that takes the form ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    SubscriptBox["x", 
     SubscriptBox["p", "n"]], "}"}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "80144881-b15d-4b2d-a535-bcf97bb38dd9"],
 StyleBox[", where ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    SubscriptBox["p", "n"], "}"}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "67a533f5-5ed4-445e-a3f6-ca5a85bd7cd1"],
 StyleBox[" is a strictly increasing sequence of positive integers.  ",
  FontWeight->"Plain"]
}], "Subsubsection",
 CellChangeTimes->{{3.428061366869833*^9, 3.428061489941071*^9}, {
  3.4281747038524637`*^9, 
  3.4281747038532467`*^9}},ExpressionUUID->"23e0e9a0-34da-4f17-be18-\
d92812e09e99"],

Cell[CellGroupData[{

Cell[TextData[{
 StyleBox["The (simplest form of the) ",
  FontWeight->"Bold"],
 StyleBox[ButtonBox["Bolzano-Weierstrass Theorem",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Bolzano-Weierstrass_Theorem"], None}],
  FontWeight->"Bold",
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 StyleBox[":",
  FontWeight->"Bold"],
 "  ",
 StyleBox["Every bounded sequence of real numbers has a convergent \
subsequence.  Moreover, if ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    SubscriptBox["x", "n"], "}"}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "3beb9b33-f37e-4b2e-bbdf-71345e1f4d4a"],
 StyleBox[" is a sequence of real numbers from the closed interval [a,b], \
then ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    SubscriptBox["x", "n"], "}"}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "32b8ed1a-71d2-4eb4-b80e-3d78f64c676b"],
 StyleBox[" has a subsequence ",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    SubscriptBox["x", 
     SubscriptBox["p", "n"]], "}"}], TraditionalForm]],
  FontWeight->"Plain",ExpressionUUID->
  "7703b264-afc1-4a02-a184-1a4970053920"],
 StyleBox[" that converges to a real number in [a,b].  ",
  FontWeight->"Plain"]
}], "Subsubsection",
 CellChangeTimes->{{3.428061631002932*^9, 3.428061716022682*^9}, {
  3.4283347143496304`*^9, 3.428334714361782*^9}, {3.428334744824185*^9, 
  3.428334748554426*^9}, {3.428334782847334*^9, 
  3.428334792290024*^9}},ExpressionUUID->"6293495d-d84b-491f-ab2a-\
3de3e6555bb6"],

Cell[TextData[{
 "All of these things, in turn, are dependent on the ",
 StyleBox[ButtonBox["Completeness Axiom",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://planetmath.org/encyclopedia/AxiomOfAnalysis.html"], None}],
  FontWeight->"Bold",
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 " (also called the Least Upper Bound Property).  This property is the most \
important property that distinguishes the field of real numbers \
\[DoubleStruckCapitalR] from the field of rational numbers \
\[DoubleStruckCapitalQ]."
}], "Text",
 CellChangeTimes->{{3.428061728539784*^9, 3.428061815597384*^9}, 
   3.61890848258252*^9},ExpressionUUID->"3d3f4957-da66-4f2e-985c-\
cf15a1db1246"]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[{
 StyleBox[ButtonBox["Completeness Axiom",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://planetmath.org/encyclopedia/AxiomOfAnalysis.html"], None}],
  FontWeight->"Bold",
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 StyleBox[": ",
  FontWeight->"Bold"],
 " ",
 StyleBox["For every nonempty set of real numbers which is bounded above, \
there exists a real number which is the least upper bound for that set.  \
(that is, there is a smallest upper bound for the set).",
  FontWeight->"Plain"]
}], "Subsubsection",
 CellChangeTimes->{{3.428061823128519*^9, 3.428061929806088*^9}, {
  3.428334971959937*^9, 
  3.428334971960767*^9}},ExpressionUUID->"087b9cd4-f009-4d53-bfcd-\
69a56b8ceec1"],

Cell[TextData[{
 "Every nonempty set of rational numbers which is bounded above also has a \
least upper bound (since every rational number is a real number).  ",
 StyleBox["However, this least upper bound need not be a rational number!",
  FontSize->14,
  FontWeight->"Bold",
  FontSlant->"Italic"],
 StyleBox[" ",
  FontSize->14],
 " In all likelihood, the least upper bound of a randomly chosen bounded set \
of rational numbers will be an ",
 StyleBox["irrational",
  FontSlant->"Italic"],
 " number (more on this when we get to section 1.4 of the textbook)."
}], "Text",
 CellChangeTimes->{{3.4280619402379713`*^9, 3.42806202436732*^9}, {
  3.4296112700694838`*^9, 
  3.429611286504801*^9}},ExpressionUUID->"3df07471-3cc9-48ac-9a42-\
287ed3efcceb"]
}, Open  ]]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["Fields", "Section",
 CellChangeTimes->{{3.4280703446577053`*^9, 
  3.428070345430539*^9}},ExpressionUUID->"d657ec77-3a6c-48b2-a3e1-\
075e0ef19a20"],

Cell[TextData[{
 "The definition of a ",
 StyleBox[ButtonBox["field",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Field_%28mathematics%29"], None}],
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 " is given in the text on page 6.  You know (from your ",
 StyleBox[ButtonBox["algebraic structures",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Algebraic_structure"], None}],
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 " days) that a field can be described more succinctly as a",
 StyleBox[" ",
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 StyleBox[ButtonBox["commutative ring",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Commutative_ring"], None}],
  FontSlant->"Italic",
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 StyleBox[ButtonBox[" ",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Commutative_ring"], None}],
  FontSlant->"Italic"],
 StyleBox["with unity in which every nonzero element is a ",
  FontSlant->"Italic"],
 StyleBox[ButtonBox["unit",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Unit_%28ring_theory%29"], None}],
  FontSlant->"Italic"],
 StyleBox["...right?  ",
  FontSlant->"Italic"],
 "Alternatively, you could also say a field is an ",
 StyleBox[ButtonBox["Abelian group",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Abelian_group"], None}],
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 " under addition whose nonzero elements form an Abelian group under \
multiplication such that the ",
 StyleBox[ButtonBox["distributive property",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Distributive_property"], None}],
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 " of multiplication over addition also holds (which is a condition for every \
ring).  "
}], "Text",
 CellChangeTimes->{{3.428070387131122*^9, 3.428070442374462*^9}, {
  3.428070478855195*^9, 3.428070499142563*^9}, {3.428070550978323*^9, 
  3.428070659270791*^9}, {3.428070701472599*^9, 3.42807070147301*^9}, {
  3.428070784802061*^9, 3.428070811765184*^9}, {3.4298706415231733`*^9, 
  3.4298706479747677`*^9}},ExpressionUUID->"c30d4c9f-8f85-427a-a418-\
770819ca1c3f"],

Cell["\<\
While all of this sounds real fancy, you should take it to mean that fields \
are essentially \"as nice as it gets\" in algebra.  Every ordinary kind of \
property that you would hope would be true, is true.\
\>", "Text",
 CellChangeTimes->{{3.428070709741972*^9, 
  3.428070756650828*^9}},ExpressionUUID->"165c6c4a-d275-447a-8c99-\
580bf88e27f5"],

Cell[TextData[{
 "The rationals \[DoubleStruckCapitalQ], the reals \[DoubleStruckCapitalR], \
and the complex numbers \[DoubleStruckCapitalC] are the most commonly used \
fields, but you will recall from algebraic structures that there are \
infinitely many others, including things like ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[DoubleStruckCapitalQ]", "(", 
     SqrtBox["2"], ")"}], "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"a", "+", 
        RowBox[{"b", 
         SqrtBox["2"]}]}], "|", " ", "a"}], ",", 
      RowBox[{"b", "\[Element]", "\[DoubleStruckCapitalQ]"}]}], "}"}]}], 
   TraditionalForm]],ExpressionUUID->"8814e92c-611d-4b2c-9eab-5f95138b10c5"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[DoubleStruckCapitalQ]", "(", 
     RadicalBox["2", "3"], ")"}], "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"a", "+", 
        RowBox[{"b", 
         RadicalBox["2", "3"]}], "+", 
        SuperscriptBox[
         RowBox[{"c", "(", 
          RadicalBox["2", "3"], ")"}], "2"]}], "|", " ", "a"}], ",", "b", ",", 
      RowBox[{"c", "\[Element]", "\[DoubleStruckCapitalQ]"}]}], "}"}]}], 
   TraditionalForm]],ExpressionUUID->"82e58cfc-36af-4001-9c1e-b1d674d741a4"],
 " and also fields.  "
}], "Text",
 CellChangeTimes->{{3.428071120481916*^9, 3.428071307807878*^9}, {
  3.6816338203893967`*^9, 
  3.6816338208974133`*^9}},ExpressionUUID->"fb5761cd-c970-419f-81c9-\
7d4741548032"],

Cell["\<\
You may be either happy or sad to know that in Real Analysis, we only concern \
ourselves with \[DoubleStruckCapitalQ] and \[DoubleStruckCapitalR] (and \
mostly just with \[DoubleStruckCapitalR]).  \
\>", "Text",
 CellChangeTimes->{{3.428071330958454*^9, 
  3.4280713708158484`*^9}},ExpressionUUID->"e40119b5-fc99-4a5e-a58a-\
f30774346f73"]
}, Open  ]],

Cell[CellGroupData[{

Cell["Ordered Sets", "Section",
 CellChangeTimes->{{3.428070352827208*^9, 
  3.428070355126483*^9}},ExpressionUUID->"a38bc986-9f9f-4383-8f69-\
9a8e8a8d3440"],

Cell[TextData[{
 "The book gives an intuitive definition of an ",
 StyleBox["order",
  FontWeight->"Bold"],
 " < on a set ",
 StyleBox["S",
  FontSlant->"Italic"],
 " on page 7, making ",
 StyleBox["S",
  FontSlant->"Italic"],
 " an ",
 StyleBox[ButtonBox["ordered set",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Ordered_set"], None}],
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 ".  In a nutshell, you want all pairs of distinct elements of ",
 StyleBox["S",
  FontSlant->"Italic"],
 " to be ordered in some way with respect to each other (",
 StyleBox[ButtonBox["trichotomy",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Trichotomy_(mathematics)"], None}],
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 ") and you want the ordering to satisfy the ",
 StyleBox[ButtonBox["transitive property",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Transitive_relation"], None}],
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 ". "
}], "Text",
 CellChangeTimes->{{3.428071973889698*^9, 3.428072023877192*^9}, {
  3.42807205539084*^9, 3.428072063724772*^9}, {3.428072229979396*^9, 
  3.428072314298627*^9}, {3.4280738525986977`*^9, 3.4280738813654346`*^9}, {
  3.428338077683522*^9, 3.428338077683942*^9}, {3.6816338845850697`*^9, 
  3.681633888365974*^9}},ExpressionUUID->"c887eeae-7bf9-48ff-95fd-\
1c2139ba96bf"],

Cell[TextData[{
 "Here, for your interest, I give the precice definition of an order on a set \
and an ordered set (based on the concept of a ",
 StyleBox[ButtonBox["relation",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Relation_%28mathematics%29"], None}],
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 ").  I also give the definition of a partial order on a set and a partially \
ordered set.  This material is not necessary for our course, but if you are \
interested you can pursue it."
}], "Text",
 CellChangeTimes->{{3.4280723522847233`*^9, 3.428072422262233*^9}, {
   3.428072461215144*^9, 3.428072502221696*^9}, 
   3.555237072354961*^9},ExpressionUUID->"4b76204e-3eaf-4842-8b06-\
a9f26b6391f8"],

Cell[TextData[{
 "Given a nonempty set ",
 StyleBox["S",
  FontSlant->"Italic"],
 ", any nonempty subset ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"R", "\[SubsetEqual]", 
     RowBox[{"S", "\[Cross]", "S"}]}], "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"a", ",", "b"}], ")"}], " ", "|", " ", "a"}], ",", 
      RowBox[{"b", "\[Element]", "S"}]}], "}"}]}], TraditionalForm]],
  ExpressionUUID->"897cda9f-8329-4820-858b-d487e13fccc2"],
 " is called a ",
 StyleBox["relation on ",
  FontWeight->"Bold"],
 StyleBox["S",
  FontWeight->"Bold",
  FontSlant->"Italic"],
 ".  A ",
 StyleBox["(strict, total) order",
  FontWeight->"Bold"],
 " ",
 StyleBox["R",
  FontSlant->"Italic"],
 " on ",
 StyleBox["S",
  FontSlant->"Italic"],
 " is a relation with the properties:  ",
 StyleBox["(1)",
  FontWeight->"Bold"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{"a", ",", "a"}], ")"}], "\[NotElement]", "R"}], 
   TraditionalForm]],ExpressionUUID->"7b6fec5d-85d3-4ff9-87a2-cfebeec99ab5"],
 " for all ",
 Cell[BoxData[
  FormBox[
   RowBox[{"a", "\[Element]", "S"}], TraditionalForm]],ExpressionUUID->
  "b6c0a7d8-7472-4d65-8c65-df584723f7f1"],
 ", ",
 StyleBox["(2)",
  FontWeight->"Bold"],
 " if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"a", ",", 
    RowBox[{"b", "\[Element]", "S"}]}], TraditionalForm]],ExpressionUUID->
  "de36bda4-86c6-46fa-9a16-82a53df3f582"],
 " with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"a", "\[NotEqual]", "b"}], TraditionalForm]],ExpressionUUID->
  "9d699c27-5f30-4688-9123-4e7322843f3f"],
 ", then either ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{"a", ",", "b"}], ")"}], "\[Element]", "R"}], TraditionalForm]],
  ExpressionUUID->"7643d0b3-a373-4117-b25e-c8fab410f4ef"],
 " or ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{"b", ",", "a"}], ")"}], "\[Element]", "R"}], TraditionalForm]],
  ExpressionUUID->"bbc3ad12-a08b-4229-8969-3b321f7f73d1"],
 " but not both (saying ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{"a", ",", "b"}], ")"}], "\[Element]", "R"}], TraditionalForm]],
  ExpressionUUID->"f91b1dae-2e53-4d8a-adc1-bc673d4a4e87"],
 " means ",
 Cell[BoxData[
  FormBox[
   RowBox[{"a", "<", "b"}], TraditionalForm]],ExpressionUUID->
  "fbb3db84-9999-4789-b64a-1222909ed293"],
 ") and ",
 StyleBox["(3)",
  FontWeight->"Bold"],
 " if ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{"a", ",", "b"}], ")"}], "\[Element]", "R"}], TraditionalForm]],
  ExpressionUUID->"f0c7dee1-1fed-47f1-999d-d10204ad1e3c"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{"b", ",", "c"}], ")"}], "\[Element]", "R"}], TraditionalForm]],
  ExpressionUUID->"4e2f0d61-7d03-4b0f-a0a8-836946f7f7dd"],
 ", then ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{"a", ",", "c"}], ")"}], "\[Element]", "R"}], TraditionalForm]],
  ExpressionUUID->"3dc7b3ef-68dd-4f19-9b3b-564a8124b3af"],
 ".  We call ",
 StyleBox["S",
  FontSlant->"Italic"],
 " with a given order ",
 StyleBox["R",
  FontSlant->"Italic"],
 " an ",
 StyleBox["ordered set",
  FontWeight->"Bold"],
 "...and we sometimes write it as the pair ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{"S", ",", "R"}], ")"}], TraditionalForm]],ExpressionUUID->
  "46918f8d-666d-4040-b702-c1eb8831114d"],
 ".  Essentially, ordered sets can be modeled as \"number lines\".  Also, the \
\"ordinary\" less than symbol <  on \[DoubleStruckCapitalR] can be modeled, \
as a relation, as a \"diagonal half-plane\" ",
 Cell[BoxData[
  FormBox[
   RowBox[{"R", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"x", ",", "y"}], ")"}], " ", "|", " ", 
      RowBox[{"x", "<", "y"}]}], "}"}]}], TraditionalForm]],ExpressionUUID->
  "8fd0fee3-1e15-46b3-8f3b-dc19a2f76511"],
 " when we think of it as a subset of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[DoubleStruckCapitalR]", "2"], "=", 
    RowBox[{
    "\[DoubleStruckCapitalR]", "\[Cross]", "\[DoubleStruckCapitalR]"}]}], 
   TraditionalForm]],ExpressionUUID->"faa2199e-b447-4de6-afa2-72b5a8d94aa4"],
 ".  "
}], "Text",
 CellChangeTimes->{{3.4280725348526363`*^9, 3.4280726186134033`*^9}, {
  3.428072714513946*^9, 3.428072870126524*^9}, {3.428073072605321*^9, 
  3.428073072606221*^9}, {3.428073581350445*^9, 3.428073699222505*^9}, {
  3.428073735597459*^9, 3.42807381540939*^9}, {3.428074028100268*^9, 
  3.4280740640805607`*^9}, {3.42807451333603*^9, 3.428074534088649*^9}, {
  3.428337789183255*^9, 3.428337840728766*^9}, {3.42987481391676*^9, 
  3.4298748463273582`*^9}, {3.429874907394223*^9, 3.4298749252740507`*^9}, {
  3.6816339480473423`*^9, 
  3.681633957652459*^9}},ExpressionUUID->"1f3f43e9-1a70-4281-bd3e-\
74525f124c5f"],

Cell[BoxData[
 RowBox[{"RegionPlot", "[", 
  RowBox[{
   RowBox[{"x", "<", "y"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", 
     RowBox[{"-", "3"}], ",", "3"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"y", ",", 
     RowBox[{"-", "3"}], ",", "3"}], "}"}], ",", 
   RowBox[{"BoundaryStyle", "\[Rule]", 
    RowBox[{"Directive", "[", "Dashed", "]"}]}]}], "]"}]], "Input",
 CellChangeTimes->{{3.428074542698496*^9, 3.428074552439289*^9}, {
  3.428074730787981*^9, 
  3.428074803311666*^9}},ExpressionUUID->"17f0e4be-1155-4960-8c5a-\
57b75574cc45"],

Cell[TextData[{
 "In the above picture, you should think of the dashed line along ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", "x"}], TraditionalForm]],ExpressionUUID->
  "ced0522c-b4e5-4d7d-aae7-fd14c75865b4"],
 " as not part of the set ",
 StyleBox["R.  ",
  FontSlant->"Italic"],
 "The dashed lines along the left side and top should be part of the set ",
 StyleBox["R",
  FontSlant->"Italic"],
 " (as should all points going off to infinity above the line ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", "x"}], TraditionalForm]],ExpressionUUID->
  "ac3ecf83-a018-4835-bb0a-0545241863e8"],
 ")."
}], "Text",
 CellChangeTimes->{{3.429874866382525*^9, 3.429874965567789*^9}, {
  3.681633985221434*^9, 
  3.681633985826497*^9}},ExpressionUUID->"d9e9df2b-dfb2-42f5-8038-\
67cd75a691e4"],

Cell[TextData[{
 "A ",
 StyleBox["(strict) partial order",
  FontWeight->"Bold"],
 " ",
 StyleBox["R",
  FontSlant->"Italic"],
 " on ",
 StyleBox["S",
  FontSlant->"Italic"],
 " is a relation that satisfies:  ",
 StyleBox["(1)",
  FontWeight->"Bold"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{"a", ",", "a"}], ")"}], "\[NotElement]", "R"}], 
   TraditionalForm]],ExpressionUUID->"8b8d3f1e-4268-4375-8336-c83580f9fb08"],
 " for all ",
 Cell[BoxData[
  FormBox[
   RowBox[{"a", "\[Element]", "S"}], TraditionalForm]],ExpressionUUID->
  "c573c663-a49e-4254-ae49-d0ca903f98fb"],
 " and ",
 StyleBox["(2)",
  FontWeight->"Bold"],
 " if ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{"a", ",", "b"}], ")"}], "\[Element]", "R"}], TraditionalForm]],
  ExpressionUUID->"19c87255-787d-484d-9def-ba6b4e50bd1a"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{"b", ",", "c"}], ")"}], "\[Element]", "R"}], TraditionalForm]],
  ExpressionUUID->"90d77be4-d226-45d4-a09b-0562274f215e"],
 ", then ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{"a", ",", "c"}], ")"}], "\[Element]", "R"}], TraditionalForm]],
  ExpressionUUID->"d8dccc9d-f112-41f7-93e7-4757891a6b37"],
 ".  A set ",
 StyleBox["S",
  FontSlant->"Italic"],
 " with a partial order defined on it is called a ",
 StyleBox["partially ordered set",
  FontWeight->"Bold"],
 ", or ",
 StyleBox[ButtonBox["poset",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Partially_ordered_set"], None}],
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 " for short.  Note the key difference with a total order that distinct \
elements need ",
 StyleBox["not",
  FontSlant->"Italic"],
 " be related for partial orders.  Any ",
 StyleBox[ButtonBox["directed graph",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Graph_(mathematics)#Directed_graph"], 
    None}],
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 " (or \[OpenCurlyDoubleQuote]digraph\[CloseCurlyDoubleQuote], for short) \
from Discrete Mathematics is an example of a partially ordered set.  In \
particular, ",
 StyleBox[ButtonBox["trees",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Tree_%28graph_theory%29"], None}],
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 " are partially ordered sets.  It is interesting that every expression in ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " can be represented as a (hierarchical) tree.      "
}], "Text",
 CellChangeTimes->{{3.42807391038099*^9, 3.428073953489356*^9}, {
   3.428074094563233*^9, 3.428074237135714*^9}, {3.428074381256549*^9, 
   3.428074381257365*^9}, {3.428074431117097*^9, 3.4280744623592863`*^9}, {
   3.428074497693142*^9, 3.4280744976935997`*^9}, {3.42807469674454*^9, 
   3.428074701413711*^9}, 3.428337870763729*^9, 3.68163400095986*^9, {
   3.681634060809498*^9, 
   3.6816340668955383`*^9}},ExpressionUUID->"e613bea9-aa35-4efe-8d01-\
9a2ceaaec889"],

