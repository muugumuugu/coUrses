Cell[TextData[{
 "For ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"-", "1"}], "\[LessEqual]", "x", "\[LessEqual]", "1"}], 
   TraditionalForm]],ExpressionUUID->"44314db6-cf94-41ba-8759-a7273223609e"],
 ", let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"g", "(", "x", ")"}], "=", 
    RowBox[{"\[LeftBracketingBar]", "x", "\[RightBracketingBar]"}]}], 
   TraditionalForm]],ExpressionUUID->"561ea4e0-2102-4646-81b4-e5fa8e8f235d"],
 " and define ",
 Cell[BoxData[
  FormBox["g", TraditionalForm]],ExpressionUUID->
  "a2e20b78-2804-4789-83a3-5551a859c725"],
 " for other values of ",
 StyleBox["x",
  FontSlant->"Italic"],
 " by \"periodically extending\" the definition over the interval ",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", 
    RowBox[{
     RowBox[{"-", "1"}], ",", "1"}], "]"}], TraditionalForm]],ExpressionUUID->
  "3f152e6a-ec19-4504-9585-ed18d3157ec6"],
 " to any other interval of length 2 whose endpoints are odd numbers.  One \
way ",
 StyleBox["g",
  FontSlant->"Italic"],
 " can be entered on ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " is shown below."
}], "Text",
 CellChangeTimes->{{3.427737573437574*^9, 3.4277376107957973`*^9}, {
  3.42773777487959*^9, 
  3.427737847499239*^9}},ExpressionUUID->"483186ce-ba69-40db-bfe1-\
d55beb3cbbee"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"g", "[", "x_", "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"EvenQ", "[", 
      RowBox[{"Floor", "[", "x", "]"}], "]"}], ",", 
     RowBox[{"Abs", "[", 
      RowBox[{"x", "-", 
       RowBox[{"Floor", "[", "x", "]"}]}], "]"}], ",", 
     RowBox[{"Abs", "[", 
      RowBox[{"FractionalPart", "[", 
       RowBox[{
        RowBox[{"Ceiling", "[", "x", "]"}], "-", "x"}], "]"}], "]"}]}], 
    "]"}]}], ";", 
  RowBox[{"Plot", "[", 
   RowBox[{
    RowBox[{"g", "[", "x", "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", 
      RowBox[{"-", "4"}], ",", "4"}], "}"}]}], "]"}]}]], "Input",
