Cell[CellGroupData[{

Cell["Ordered Fields", "Section",
 CellChangeTimes->{{3.428070366621984*^9, 
  3.4280703686225557`*^9}},ExpressionUUID->"b1b7447d-e481-4247-91ef-\
20c8d6249694"],

Cell[TextData[{
 "In the notation of the preceding section, an ",
 StyleBox[ButtonBox["ordered field",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://en.wikipedia.org/wiki/Ordered_field"], None}],
  FontVariations->{"Underline"->True},
  FontColor->RGBColor[1, 0, 1]],
 " is a field ",
 StyleBox["F",
  FontSlant->"Italic"],
 " on which a strict total order ",
 StyleBox["R",
  FontSlant->"Italic"],
 " has been defined which also satisfies the following properties:  ",
 StyleBox["(1)",
  FontWeight->"Bold"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{"0", ",", "x"}], ")"}], ",", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"0", ",", "y"}], ")"}], "\[Element]", 
     RowBox[{"R", "\[DoubleLongRightArrow]", 
      RowBox[{"(", 
       RowBox[{"0", ",", 
        RowBox[{"x", "+", "y"}]}], ")"}]}], "\[Element]", "R"}]}], 
   TraditionalForm]],ExpressionUUID->"2a97c921-9c07-455d-8026-81aee1fc80a5"],
 ", ",
 StyleBox["(2)",
  FontWeight->"Bold"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{"0", ",", "x"}], ")"}], ",", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"0", ",", "y"}], ")"}], "\[Element]", 
     RowBox[{"R", "\[DoubleLongRightArrow]", 
      RowBox[{"(", 
       RowBox[{"0", ",", 
        RowBox[{"x", " ", "y"}]}], ")"}]}], "\[Element]", "R"}]}], 
   TraditionalForm]],ExpressionUUID->"e6c1760a-8f25-4708-bf45-c1597045d5c5"],
 ", and ",
 StyleBox["(3)",
  FontWeight->"Bold"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{"x", ",", "y"}], ")"}], "\[Element]", 
    RowBox[{"R", "\[DoubleLongLeftRightArrow]", 
     RowBox[{"(", 
      RowBox[{"0", ",", 
       RowBox[{"y", "-", "x"}]}], ")"}]}], "\[Element]", "R"}], 
   TraditionalForm]],ExpressionUUID->"e05d0938-d8cc-4fb6-8f11-1424f62103cd"],
 " (compare these to the book's statements on page 8).    "
}], "Text",
 CellChangeTimes->{{3.428074842140183*^9, 3.428074915539317*^9}, {
  3.428074949758903*^9, 3.4280750979106007`*^9}, {3.4280751331386147`*^9, 
  3.4280751593107347`*^9}, {3.4283380283362503`*^9, 
  3.4283380283465977`*^9}},ExpressionUUID->"de7a688f-f635-4b81-89b5-\
27307681a139"]
}, Open  ]],

