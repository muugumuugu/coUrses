Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "x", ")"}], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"k", "=", "0"}], "\[Infinity]"], 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox["3", "4"], ")"}], "k"], 
      RowBox[{"g", "(", 
       RowBox[{
        SuperscriptBox["4", "k"], "x"}], ")"}]}]}]}], TraditionalForm]]],
