Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"d", "[", 
    RowBox[{"x_", ",", "y_"}], "]"}], ":=", 
   RowBox[{"Abs", "[", 
    RowBox[{"x", "-", "y"}], "]"}]}], ";", 
  RowBox[{"Manipulate", "[", 
   RowBox[{
