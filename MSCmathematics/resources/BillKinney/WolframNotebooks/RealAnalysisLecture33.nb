          RowBox[{
           RowBox[{"d", "[", 
            RowBox[{"x", ",", 
             RowBox[{"a", "-", "b"}]}], "]"}], "+", 
           RowBox[{"d", "[", 
            RowBox[{"b", ",", "0"}], "]"}]}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"x", ",", 
          RowBox[{"-", "10"}], ",", "10"}], "}"}], ",", 
