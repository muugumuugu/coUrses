Cell[TextData[{
 "For ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"-", "1"}], "\[LessEqual]", "x", "\[LessEqual]", "1"}], 
   TraditionalForm]]],
 ", let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"g", "(", "x", ")"}], "=", 
    RowBox[{"\[LeftBracketingBar]", "x", "\[RightBracketingBar]"}]}], 
   TraditionalForm]]],
 " and define ",
 Cell[BoxData[
  FormBox["g", TraditionalForm]]],
 " for other values of ",
 StyleBox["x",
  FontSlant->"Italic"],
 " by \"periodically extending\" the definition over the interval ",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", 
    RowBox[{
     RowBox[{"-", "1"}], ",", "1"}], "]"}], TraditionalForm]]],
 " to any other interval of length 2 whose endpoints are odd numbers.  One \
way ",
 StyleBox["g",
  FontSlant->"Italic"],
 " can be entered on ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " is shown below."
}], "Text",
 CellChangeTimes->{{3.427737573437574*^9, 3.4277376107957973`*^9}, {
  3.42773777487959*^9, 3.427737847499239*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"g", "[", "x_", "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"EvenQ", "[", 
      RowBox[{"Floor", "[", "x", "]"}], "]"}], ",", 
     RowBox[{"Abs", "[", 
      RowBox[{"x", "-", 
       RowBox[{"Floor", "[", "x", "]"}]}], "]"}], ",", 
     RowBox[{"Abs", "[", 
      RowBox[{"FractionalPart", "[", 
       RowBox[{
        RowBox[{"Ceiling", "[", "x", "]"}], "-", "x"}], "]"}], "]"}]}], 
    "]"}]}], ";", 
  RowBox[{"Plot", "[", 
   RowBox[{
    RowBox[{"g", "[", "x", "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", 
      RowBox[{"-", "4"}], ",", "4"}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.4277363203915367`*^9, 3.427736330543722*^9}, {
  3.427736372197502*^9, 3.427736373901308*^9}, {3.427736520162388*^9, 
  3.427736579727536*^9}, {3.427736623049081*^9, 3.4277366359796467`*^9}, {
  3.427736710021517*^9, 3.427736712195157*^9}, {3.4277367643808804`*^9, 
  3.427736785181788*^9}}],

Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "x", ")"}], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"k", "=", "0"}], "\[Infinity]"], 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox["3", "4"], ")"}], "k"], 
      RowBox[{"g", "(", 
       RowBox[{
        SuperscriptBox["4", "k"], "x"}], ")"}]}]}]}], TraditionalForm]]],
 ".  It turns out that ",
 StyleBox["f",
  FontSlant->"Italic"],
 " is ",
 StyleBox["continuous everywhere but differentiable nowhere",
