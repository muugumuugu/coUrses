             RowBox[{"f", "[", 
              RowBox[{"c", "/.", "sol"}], "]"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"a", ",", "0"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"b", ",", "0"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"c", "/.", "sol"}], ",", "0"}], "}"}]}], "}"}], ",", 
         RowBox[{"PlotStyle", "\[Rule]", 
          RowBox[{"PointSize", "[", ".02", "]"}]}]}], "]"}], ",", 
